function [ A ] = gridShift( A, x, y )
%GRIDSHIFT Summary of this function goes here
%   Detailed explanation goes here

sizex = size(A,1);
sizey = size(A,2);

for i=1:sizex
    if ii>sizex
        continue
    end
    for j=1:sizey
        yy = y + y;
        if yy>sizey
            continue
        end
        
        A
    end
end

end

